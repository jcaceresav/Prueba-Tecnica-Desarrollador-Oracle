  CREATE TABLE "DVPARTNERS"."PERSONAS" 
   (	"ID" NUMBER NOT NULL ENABLE, 
	"NOMBRES" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"APELLIDOS" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"NUMEROIDENTIFICACION" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
